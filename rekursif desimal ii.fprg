    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="y, x" type="Integer" array="False" size=""/>
            <output expression="&quot;Masukkan angka yang anda inginkan&quot;" newline="True"/>
            <input variable="y"/>
            <for variable="x" start="1" end="y" direction="inc" step="1">
                <output expression="desimal (x*3)" newline="True"/>
            </for>
        </body>
    </function>
    <function name="desimal" type="Integer" variable="x">
        <parameters>
            <parameter name="x" type="Integer" array="False"/>
        </parameters>
        <body>
            <if expression="x=1">
                <then>
                    <assign variable="x" expression="2"/>
                </then>
                <else>
                    <assign variable="x" expression="(x*2)+1"/>
                </else>
            </if>
        </body>
    </function>
</flowgorithm>
